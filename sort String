class Solution 
{ 
    String sort(String s) 
    {
        // code here
        String str = "";
        for(char ch = 'a' ; ch <= 'z' ; ch++){
            for(int i = 0 ; i < s.length() ; i++){
                if(ch == s.charAt(i)){
                    str = str + s.charAt(i);
                }
            }
        }
        return str;
    }
}
