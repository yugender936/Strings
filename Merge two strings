class Solution 
{ 
    String merge(String S1, String S2)
    { 
        // code here
        String str = "";
        for(int i = 0 ; i < S1.length() || i < S2.length() ; i++){
            if(i < S1.length()){
                str = str + S1.charAt(i);
            }
            if(i < S2.length()){
                str = str + S2.charAt(i);
            }
        }
        return str;
    }
}
